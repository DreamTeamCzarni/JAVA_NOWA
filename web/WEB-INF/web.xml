<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>ApteczkaServlet</servlet-name>
        <servlet-class>ApteczkaServlet.ApteczkaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DodawanieServlet</servlet-name>
        <servlet-class>ApteczkaServlet.DodawanieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuwanieServlet</servlet-name>
        <servlet-class>ApteczkaServlet.UsuwanieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WyswietlanieServlet</servlet-name>
        <servlet-class>ApteczkaServlet.WyswietlanieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WyswietlAlfabetycznieServlet</servlet-name>
        <servlet-class>ApteczkaServlet.WyswietlAlfabetycznieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ApteczkaServlet</servlet-name>
        <url-pattern>/start</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DodawanieServlet</servlet-name>
        <url-pattern>/dodawanie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuwanieServlet</servlet-name>
        <url-pattern>/usuwanie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WyswietlanieServlet</servlet-name>
        <url-pattern>/wyswietlanie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WyswietlAlfabetycznieServlet</servlet-name>
        <url-pattern>/wyswietlalfabetycznie</url-pattern>
    </servlet-mapping>
</web-app>